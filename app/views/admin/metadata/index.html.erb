<h1>Metadata Records</h1>

<table>
  <thead>
  <tr>
    <th>ID</th>
    <th>Filename</th>
    <th>User ID</th>
    <th>Actions</th>
  </tr>
  </thead>

  <tbody>
  <% @records.each do |record| %>
    <tr>
      <td><%= record.id %></td>
      <td><%= record.original_filename %></td>
      <td><%= record.user_id %></td>
      <td>
        <%= link_to "View", admin_metadata_path(record) %> |
        <%= link_to "Edit", edit_admin_metadata_path(record) %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>
